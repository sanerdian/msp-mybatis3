<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jnetdata.msp.zdff.publishdistribution.mapper.PublishdistriButionMapper">

    <select id="findPublishdistriBution" resultType="com.jnetdata.msp.zdff.publishdistribution.model.PublishdistriBution">
